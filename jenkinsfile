
pipeline {
	agent any
	
	stages {
		stage('Build'){
			steps {
				echo 'Build ...'
				sh 'mvn install -Dmaven.test.skip=true'
				echo "ls ${env.WORKSPACE}/target/*.jar"
			}
		}
		stage('Test') {
			steps {
				echo 'Testing...'
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying...'
				//sh 'java -jar ${env.WORKSPACE}/target/*.jar'
			}
		}
	
	}
}
